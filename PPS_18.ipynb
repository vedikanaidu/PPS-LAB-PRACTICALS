        "# Type Content here...\n",
        "def is_palindrome(s):\n",
        "\treturn s == s[::-1]\n",
        "\n",
        "s = input()\n",
        "\n",
        "if is_palindrome(s):\n",
        "\tprint(\"palindrome\")\n",
        "else:\n",
        "\tprint(\"not a palindrome\")"
      ]
