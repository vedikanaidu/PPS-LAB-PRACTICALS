        "import string\n",
        "\n",
        "def remove_punctuation(s):\n",
        "\treturn ' '.join(''.join(char for char in s if char not in string.punctuation).split())\n",
        "\n",
        "s = input()\n",
        "\n",
        "print(remove_punctuation(s))"
      ]
    }
