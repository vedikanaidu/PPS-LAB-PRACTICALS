        "def Sumof(n):\n",
        "\tif n == 0:\n",
        "\t\treturn 0\n",
        "\telse:\n",
        "\t\treturn(n % 10) + Sumof(n // 10)\n",
        "n = int(input())\n",
        "print(Sumof(n))"
      ]
    }
