      "source": [
        "def is_prime(n):\n",
        "\tif n < 2:\n",
        "\t\treturn False\n",
        "\tfor i in range(2, int(n ** 0.5) + 1):\n",
        "\t\tif n % i == 0:\n",
        "\t\t\treturn False\n",
        "\treturn True\n",
        "\n",
        "upper_limit = int(input(\"Enter upper limit: \"))\n",
        "for num in range(2, upper_limit):\n",
        "\tif is_prime(num):\n",
        "\t\tprint(num)"
      ]
    }
  ]
}