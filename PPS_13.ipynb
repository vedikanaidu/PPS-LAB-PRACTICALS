        "def print_pattern(n):\n",
        "\tfor i in range(n, 0, -1):\n",
        "\t\tprint(\"* \"*i)\n",
        "\n",
        "number = int(input(\"Enter a number : \"))\n",
        "\n",
        "print_pattern(number)"
